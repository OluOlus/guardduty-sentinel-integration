{
  "name": "guardduty-sentinel-integration",
  "version": "1.0.0",
  "description": "End-to-end AWS GuardDuty to Azure Sentinel integration system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts tests/**/*.ts",
    "format:check": "prettier --check src/**/*.ts tests/**/*.ts"
  },
  "keywords": [
    "aws",
    "guardduty",
    "azure",
    "sentinel",
    "security",
    "integration"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@types/aws-lambda": "^8.10.122",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fast-check": "^3.15.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@azure/functions": "^4.0.0",
    "@azure/identity": "^4.0.0",
    "@azure/monitor-ingestion": "^1.0.0",
    "ajv": "^8.17.1",
    "aws-lambda": "^1.0.7",
    "express": "^4.19.2",
    "js-yaml": "^4.1.1"
  }
}
